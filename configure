if test x$RESFALLBACK != x; then
   echo "  resource class fallback:    $RESFALLBACK"
fi
echo
echo "  embedded perl:              $support_perl"
echo "  libafterimage:              $support_afterimage"
echo
if test x$support_xim = xyes -a x$rxvt_cv_func_xlocale = xno; then
